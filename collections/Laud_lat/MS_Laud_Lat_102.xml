               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="perg">
                        <support>parchment</support>
                        <extent>ii + 210 + ii
                           <dimensions unit="mm" type="leaf">
                              <height max="298" min="288">c. 288-98</height>
