               <head>Sulpicius Severus, Life of S. Martin, and other saints' lives; 12th century, end Germany, Cistercian abbey of Eberbach</head>
               <msContents>
                  
                  <msItem xml:id="MS_Laud_Misc_538-item1" n="1">
                     <locus>(fol. 1r-73r)</locus>
                     <author key="person_30332452">Sulpicius Severus</author>
                     <title key="work_4632">Vita S. Martini</title>
                     <rubric>Incipit prefatio Severi in vita S. Martini</rubric>
                     <finalRubric>Explicit dyalogus Seueri tertius de vita S. Martini episcopi</finalRubric>
                     <textLang mainLang="la">Latin</textLang>
                    
<!--                    Decide how to deal with a-f, current numbering/lettering based on Coxe-->
                     <msItem n="a">
                        <locus>(fols. 73r-74r)</locus>
                        <rubric>Sermo beati Martini episcopi de S. Trinitate</rubric>
                        <incipit>Clemens Trinitas est una Divinitas; ut autem per Sacramentum cognoscitur </incipit>
                     </msItem>
                     
                     <msItem n="b">
                        <locus>(fols. 74r-75r)</locus>
                        <rubric>Incipit epistola Gregorii Turonensis episcopi de transitu S. Martini episcopi</rubric>
                        <incipit>Archadio igitur et Honorio</incipit>
                     </msItem>
                     
                     <msItem n="c">
                        <locus>(fols. 75r-76r)</locus>
                        <rubric>Ejusdem alia de obitu S. Martini</rubric>
                        <incipit>Beatus autem Severinus Coloniensis </incipit>
                     </msItem>
                     
                     <msItem n="d">
                        <locus>(fol. 76r-v)</locus>
                        <rubric>Alia ejusem de transitu S. Martini</rubric>
                        <incipit>Eo namque tempore </incipit>
                     </msItem>
                     
                     <msItem n="e">
                        <locus>(fols. 76v-78r)</locus>
                        <rubric>Sermo ipsius de translatione corporis S. Martini </rubric>
                        <incipit>Opere precium est enim </incipit>
                     </msItem>
                     
                     <msItem n="f">
                        <locus>(fols. 78r-82r)</locus>
                        <rubric>Epilogus mastiri Alhcwini de miraculis beati Martini episcopi</rubric>
                        <incipit>Postquam Dominus noster Jhesus Christus </incipit>
                     </msItem>
                  </msItem>
                  
                  <msItem xml:id="MS_Laud_Misc_538-item2" n="2">
                     <locus>(fols. 82r-84r)</locus>
                     <author key="person_95289837">Gregory of Tours</author>
<!--                     CHK BHL - does it circulate on its own?-->
                     <title type="desc">Vita S. Brictii</title>
                     <rubric>Incipit uita sancti Brictii episcopii</rubric>
                     <incipit>Igitur post excessum </incipit>
                     <note>According to Gregory of Tours, <title key="work_1859">Historia Francorum</title>.</note>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  
                  <msItem xml:id="MS_Laud_Misc_538-item3" n="3">
                     <locus>(fol. 84r-86r)</locus>
                     <author key="person_40174477">Paul the Deacon</author>
                     <title key="work_3457">Life of S. Gregory</title>
                     <note>an abbreviation</note>
                     <rubric>Sermo de gestis beati Gregorii pape</rubric>
                     <incipit>Gregorius igitur genere Romanus, patre Gordiano, matre vero Silvia editus </incipit>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  
                  <msItem>
                     <locus>(fol. 87r)</locus>
                     <title key="work_13878">Marian poem in German</title>
                     <rubric>O maria mater</rubric>
                     <incipit>Maria muter v<gap/> <lb/> Din liebe kein<gap/> <lb/> Wir sin die arme<gap/> </incipit>
                     <explicit> want sie ist aller genaden ful </explicit>
                     <note>Added, 15th century.</note>
                     <note>Pr. in Palmer 1998, p. 216.</note>
                     <textLang mainLang="de">German</textLang>
                  </msItem>
                  
